
.jScrollPaneHorizontalContainer {
	position: relative;
	overflow: hidden;
	z-index: 1;
}

.jScrollPaneHorizontalTrack {
	position: absolute;
	cursor: pointer;
	left: 0;
	bottom: 0;
	width: 100%;
	background: #aaa;
}

.jScrollIntervalTrackHorizontal { 
	position: absolute;

}

.jScrollPaneHorizontalDrag {
	position: absolute;
	background: #666;
	cursor: pointer;
	overflow: hidden;
}
.jScrollPaneHorizontalDragLeft {
	position: absolute;
	bottom: 0;
	left: 0;
	overflow: hidden;
}
.jScrollPaneHorizontalDragRight {
	position: absolute;
	bottom: 0;
	right: 0;
	overflow: hidden;
}
a.jScrollArrowLeftHorizontal {
	display: block;
	position: absolute;
	z-index: 1;
	bottom: 0;
	left: 0;
	text-indent: -2000px !important;
	overflow: hidden;
	/*background-color: #666;*/
	height: 9px;
	width: 10px;
}
a.jScrollArrowLeftHorizontal:hover {
	/*background-color: #f60;*/
}

a.jScrollArrowRightHorizontal {
	display: block;
	position: absolute;
	z-index: 1;
	bottom: 0;
	right: 0;
	text-indent: -2000px !important;
	overflow: hidden;
	/*background-color: #666;*/
	height: 9px;
	width: 10px;
}
a.jScrollArrowRightHorizontal:hover {
	/*background-color: #f60;*/
}
a.jScrollActiveArrowButtonHorizontal, a.jScrollActiveArrowButtonHorizontal:hover {
