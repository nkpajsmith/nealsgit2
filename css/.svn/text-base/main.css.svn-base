}

span.action-log a:link span, span.action-log a:visited span {
    float: left;
    display: block;
    margin: 0 -9px 0 0;
    height: 31px;
    line-height: 31px;
    padding: 0 33px;
    background: url(../images/bg-action-log-right.png) top right no-repeat;
    cursor: pointer;
}

/* Page Container */
