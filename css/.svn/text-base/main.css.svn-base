/* Tooltip */
#cluetip {
    background-color: #70a3c0;
    padding: 20px;
    width: 210px;
    color: #fff;
    font-size: 12px;
    border-radius: 10px;
    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
}
#cluetip #cluetip-close {
    font-size: 12px;
}
#cluetip a:link, #cluetip a:visited {
    color: #cff5ff;
    text-decoration: none;
}
#cluetip h3 {
    color: #fff;
    margin: 0 0 5px 0;
    padding: 0 0 0 0;
    font-size: 14px;
