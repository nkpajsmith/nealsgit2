/* Tab Panel */
div.tab-panel, div.tab-panel-b {padding: 0 0 0 14px;}
div.layout-b div.tab-panel {
    padding: 0 0 0 36px;
    position: relative;
    top: 6px;
}
div.layout-b div.tab-panel1 {
    background:url("../images/bg-main-column-top.png") no-repeat scroll left top #FFFFFF;
    padding:0 0 0 36px;
