}
div.tab-panel ol li span {
   font-size: 100%; 
   color: #000000;
   margin: 5; 
  font-weight:normal
}

/* TP Progress Next */
div.tp-progress {
    position: relative;
    height: 1%;
}

div.tp-progress ul li.next {
    background: url(../images/bg-tp-progress-next.gif) bottom center no-repeat;
    position: relative;
    top: 7px;
    margin: -7px 0 0 0;
    padding: 0 2px 10px 0!important;
    border-bottom: none;
}
div.tp-progress ul li.last {
    padding-right: 0!important;
}

div.tp-progress ul li.next a:link, div.tp-progress ul li.next a:visited {
    border-bottom: 1px solid #d6d6d6;
}

div.layout-b div.tab-panel ul li {
    padding: 0 2px 1px 0;
    border-bottom: none;
    background: url(../images/bg-tab-line.gif) bottom left no-repeat;
}

div.tab-panel ul li.last, div.table-panel-b ul li.last {padding: 0;}
div.layout-b div.tab-panel ul li.last {padding: 0 0 1px 0; margin: 0 -3px 0 0;}

div.tab-panel ul li a:link, div.tab-panel ul li a:visited,
div.tab-panel-b ul li a:link, div.tab-panel-b ul li a:visited {
    float: left;
    color: #aaa9a9;
    text-decoration: none;
    background: url(../images/bg-tab-left.gif) top left no-repeat;
}
div.layout-b div.tab-panel ul li a:link, div.layout-b div.tab-panel ul li a:visited {
    background: url(../images/bg-tab-left-b.gif) top left no-repeat;
}

div.tab-panel ul li a:link span, div.tab-panel ul li a:visited span {
    display: block;
    float: left;
    padding: 8px 29px 5px 29px;
    background: url(../images/bg-tab-right.gif) top right no-repeat;
    cursor: pointer;
}

div.tab-panel-b ul li a:link span, div.tab-panel-b ul li a:visited span {
    display: block;
    float: left;
    padding: 8px 20px 5px 20px;
    background: url(../images/bg-tab-right.gif) top right no-repeat;
    cursor: pointer;
}

div.layout-b div.tab-panel ul li a:link span, div.layout-b div.tab-panel ul li a:visited span {
    background: url(../images/bg-tab-right-b.gif) top right no-repeat;
}

/* TP Progress Links */
div.tp-progress ul li a:link span, div.tp-progress ul li a:visited span {
    padding: 8px 18px 5px 18px;
}

/* TP Progress Label */
div.tp-progress p.tp-progress-label {
    position: absolute;
    top: 45px;
    left: 122px;
    margin: 0;
    font-size: 85%;
}

/* Active Tab Style */
div.tab-panel ul li.active, div.tab-panel-b ul li.active {
    border-bottom: 1px solid #FFF;
}

/* TP Progress Active */
div.tp-progress ul li.active {
    position: relative;
    top: 7px;
    margin: -7px 0 0 0;
    padding: 0 2px 10px 0;
    background: #FFF;
}

div.tp-progress ul li.done {
    position: relative;
    top: 7px;
    margin: -7px 0 0 0;
    padding: 0 0 11px 0;
    background: #FFF;
    border-bottom: none;
}

div.tp-progress ul li.done a:link, div.tp-progress ul li.done a:visited {
    border-bottom: none;
}

div.layout-b div.tab-panel ul li.active {
    border-bottom: none;
    background: url(../images/bg-tab-line-active.gif) bottom right no-repeat;
}

div.tab-panel ul li.active a:link, div.tab-panel ul li.active a:visited,
div.tab-panel-b ul li.active a:link, div.tab-panel-b ul li.active a:visited {
    color: #000;
    background: url(../images/bg-tab-left-active.gif) top left no-repeat !important;
}
div.layout-b div.tab-panel ul li.active a:link, div.layout-b div.tab-panel ul li.active a:visited {
    color: #aaa9a9;
    background: url(../images/bg-tab-left-active-b.gif) top left no-repeat;
}
div.tab-panel-sections ul li.active a:link, div.tab-panel-sections ul li.active a:visited {
    color: #000!important;
}

div.layout-b div.tab-panel ul li.completed a:link, div.layout-b div.tab-panel ul li.completed a:visited {
    color: #000;
}

div.tab-panel ul li.active a:link span, div.tab-panel ul li.active a:visited span,
div.tab-panel-b ul li.active a:link span, div.tab-panel-b ul li.active a:visited span {
    background: url(../images/bg-tab-right-active.gif) top right no-repeat;
}
div.layout-b div.tab-panel ul li.active a:link span, div.layout-b div.tab-panel ul li.active a:visited span {
    background: url(../images/bg-tab-right-active-b.gif) top right no-repeat;
}

/* Tab Content */
div.tab-content {
    padding: 55px 23px 0 23px;
    clear: both;
}

div.tab-content p {
    color: #454545;
    font-size: 93%;
}

div.tab-content div.section {
    padding: 0 0 56px 4px;
}

/* Data Chart - At a Glance */
ul.at-a-glance {
    width: 267px;
    float: left;
    margin: 0 52px 0 0;
    border-top: 1px solid #a09f9f;
}

ul.at-a-glance li {
    border-bottom: 1px solid #a09f9f;
    list-style: none;
    padding: 11px 0;
}

/* Auto Clear List Items */
ul.at-a-glance li:after {
    content: "."; 
    display: block; 
    height: 0; 
    clear: both; 
    visibility: hidden;
}

ul.at-a-glance li {display: inline-block;}

/* Hides from IE-mac \*/
* html ul.at-a-glance li {height: 1%;}
ul.at-a-glance li {display: block;}
/* End hide from IE-mac */


