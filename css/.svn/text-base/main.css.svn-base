}

div.tab-panel ul, div.tab-panel-b ul{margin: 0;}

div.tp-progress ul {
    border-bottom: 7px solid #fec408;
    float: left;
}

div.tab-panel ul li, div.tab-panel-b ul li{
    list-style: none;
    float: left;
    font-size: 93%;
