}

div.fw-section-b-inner {
    padding: 20px 14px 28px 14px;
}

div.fw-section-inner, div.fw-form-section-inner1 {
    background:url("../images/bg-main-column-bottom.png") no-repeat scroll left bottom #FFFFFF;
    height:1%;
